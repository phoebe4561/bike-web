@using PagedList
@using PagedList.Mvc
@model IPagedList<bike_web.ViewModels.PrivateRouteData>


<link href="~/Content/PagedList.css" rel="stylesheet" />



@{
    ViewBag.Title = "privateRoute";
}

<section class="page-section" id="contact">
    <div class="container">

        <div class="row">
            @foreach (var item in Model)
            {
                @Html.Partial("_PrivateRouteCard", item)

            }
  
        </div>



        <div class="row">
                <div class="col">
                    <nav aria-label="Page navigation example">
                        <ul class="pagination justify-content-center">
                         @Html.PagedListPager((IPagedList)Model, x => Url.Action("privateRoute", new { page = x }))   
                        </ul>
                    </nav>
                </div>
            </div>
    </div>
</section>




